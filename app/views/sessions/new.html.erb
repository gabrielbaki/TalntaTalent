<div class="header">
  <div class="container">
    <%= image_tag("talntalogo.jpg", :alt => "talntalogo feed") %>
    <h1>Talnta New Talent</h1>
  </div>
</div>

<div class="create">
  <div class="container">

    <!-- Your code here -->
    <%= form_for(:session, url: login_path) do |f| %>
    <%= f.email_field :email, :placeholder => "Email" %>
    <%= f.password_field :password, :placeholder => "Password" %>
    <%= f.submit "Log in", class: "btn-submit" %>
  <% end %>
<br>
<%= button_to "Sign Up", 'signup', :class => "button", :method => :get  %>

  </div>
</div>
